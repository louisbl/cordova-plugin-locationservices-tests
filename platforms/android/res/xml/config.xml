<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.louisbl.cordova.tests.locationservices" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="LocationServices">
        <param name="android-package" value="fr.louisbl.cordova.locationservices.CordovaLocationServices" />
    </feature>
    <allow-intent href="market:*" />
    <name>TestsLocationServices</name>
    <description>
        A simple Apache Cordova application that tests the cordova-plugin-locationservices.
    </description>
    <author email="louis@beltramo.me" href="http://louisbl.fr">
        louisbl
    </author>
    <content src="cdvtests/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
